{
  "name": "telegram-bot-qazlatyn",
  "version": "0.3.0",
  "description": "Telegram bot for transform old Kazakh characters to new",
  "repository": {
    "type": "git",
    "url": "https://github.com/drugoi/telegram-bot-qazlatyn.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "start:prod": "npm run build && node dist/index.js",
    "test": "ava",
    "start:dev": "concurrently -i -k -p \"[{name}]\" -n \"Node, TypeScript\" -c \"yellow.bold,cyan.bold\" \"npm run watch-js\" \"npm run watch-ts\"",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/index.js",
    "build": "tsc --skipLibCheck"
  },
  "author": "Nikita Bayev <nikita@bayev.kz> (https://bayev.kz/)",
  "license": "MIT",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.14.6",
    "dotenv": "^8.2.0",
    "qazlatyn-db": "^3.0.1",
    "telegraf": "^3.38.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.1.2"
  }
}
